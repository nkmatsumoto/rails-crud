<%= form_with model: restaurant do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :address %>
  <%= f.text_field :address, class: "form-control" %>
  <%= f.label :rating %>
  <%= f.number_field :rating, class: "form-control" %>
  <%= f.submit "Submit", class: "btn btn-success mt-3" %>
<% end %>
